[flake8]
max-line-length = 160
ignore =
    # E203: whitespace before ':'.
    E203,
    # W503: line break before binary operator.
    W503
per-file-ignores =
    # F401: module imported but unused.
    # F403: 'from module imkport *' used; unable to detect undefined names
    __init__.py: F401,F403
